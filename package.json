{
  "name": "@ngyewch/leaflet-webcomponent",
  "version": "0.4.0",
  "description": "Leaflet web component",
  "keywords": [
    "leaflet",
    "geojson",
    "web-component"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngyewch/leaflet-webcomponent.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/leaflet-webcomponent.js",
      "require": "./dist/leaflet-webcomponent.umd.cjs",
      "script": "./dist/leaflet-webcomponent.umd.cjs",
      "types": "./dist/leaflet-webcomponents.d.ts"
    }
  },
  "module": "./dist/leaflet-webcomponent.js",
  "main": "./dist/leaflet-webcomponent.umd.cjs",
  "types": "./dist/leaflet-webcomponents.d.ts",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "postbuild": "validate-package-exports --check",
    "prepublishOnly": "validate-package-exports --check"
  },
  "devDependencies": {
    "@types/geojson": "7946.0.14",
    "@types/leaflet": "1.9.12",
    "geojson": "0.5.0",
    "leaflet": "1.9.4",
    "lit": "3.2.0",
    "typescript": "5.6.2",
    "validate-package-exports": "0.6.1",
    "vite": "5.4.5",
    "vite-plugin-dts": "4.2.1"
  },
  "dependencies": {
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
